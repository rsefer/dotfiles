#!/bin/sh
#
# Get featured Contacts

osascript -e "tell application \"Contacts\"" \
	-e "set featuredPeople to {}" \
	-e "repeat with p in people in group \"Featured\"" \
		-e "copy {nickname of p, value of phone 1 of p} to the end of featuredPeople" \
	-e "end repeat" \
	-e "return featuredPeople" \
-e "end tell"
