{
	"[go]": {
		"editor.defaultFormatter": "golang.go",
		"editor.formatOnSave": true,
		"editor.insertSpaces": true
	},
	"aws.codeWhisperer.importRecommendation": false,
	"aws.suppressPrompts": {
		"codeWhispererConnectionExpired": true,
		"codeWhispererNewWelcomeMessage": true
	},
	"aws.telemetry": false,
	"bookmarks.useWorkaroundForFormatters": true,
	"breadcrumbs.enabled": false,
	"breadcrumbs.symbolSortOrder": "type",
	"css.format.preserveNewLines": false,
	"css.format.spaceAroundSelectorSeparator": true,
	"css.lint.compatibleVendorPrefixes": "warning",
	"css.lint.duplicateProperties": "warning",
	"css.lint.float": "warning",
	"css.lint.hexColorLength": "warning",
	"csv-preview.capitalizeHeaders": false,
	"debug.javascript.autoAttachFilter": "smart",
	"debug.toolBarLocation": "docked",
	"diffEditor.experimental.showMoves": true,
	"diffEditor.hideUnchangedRegions.enabled": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"diffEditor.renderSideBySide": false,
	"editor.accessibilitySupport": "off",
	"editor.bracketPairColorization.enabled": true,
	"editor.codeLens": false,
	"editor.colorDecorators": false,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.fontSize": 18,
	"editor.guides.bracketPairs": true,
	"editor.hover.delay": 2500,
	"editor.hover.enabled": true,
	"editor.inlineSuggest.enabled": true,
	"editor.linkedEditing": true,
	"editor.minimap.enabled": false,
	"editor.minimap.maxColumn": 25,
	"editor.minimap.renderCharacters": false,
	"editor.minimap.size": "fit",
	"editor.parameterHints.enabled": false,
	"editor.renderLineHighlight": "all",
	"editor.renderWhitespace": "selection",
	"editor.scrollBeyondLastLine": false,
	"editor.smoothScrolling": true,
	"editor.stickyScroll.enabled": true,
	"editor.tabCompletion": "on",
	"editor.tabSize": 2,
	"editor.wordWrap": "on",
	"editor.wordWrapColumn": 120,
	"emmet.includeLanguages": {
		"javascript": "javascriptreact"
	},
	"emmet.syntaxProfiles": {
		"html": {
			"attr_quotes": "double"
		}
	},
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"extensions.ignoreRecommendations": true,
	"figma.autocompleteProperties": false,
	"files.associations": {
		"*.njk": "html"
	},
	"files.defaultLanguage": "markdown",
	"files.hotExit": "onExitAndWindowClose",
	"files.insertFinalNewline": true,
	"files.trimFinalNewlines": true,
	"git.autofetch": true,
	"git.blame.editorDecoration.enabled": true,
	"git.blame.statusBarItem.enabled": true,
	"git.confirmSync": false,
	"git.enableSmartCommit": true,
	"git.openRepositoryInParentFolders": "always",
	"githubIssues.issueBranchTitle": "${user}/${issueNumber}-${sanitizedIssueTitle}",
	"githubIssues.queries": [
		{
			"label": "Open Issues",
			"query": "repo:${owner}/${repository} state:open sort:created-desc"
		}
	],
	"githubPullRequests.createOnPublishBranch": "never",
	"githubPullRequests.pullBranch": "never",
	"githubPullRequests.pushBranch": "always",
	"githubPullRequests.queries": [
		{
			"label": "Local Pull Request Branches",
			"query": "default"
		},
		{
			"label": "All Open",
			"query": "default"
		}
	],
	"gitlens.codeLens.enabled": false,
	"gitlens.currentLine.enabled": false,
	"gitlens.hovers.currentLine.over": "line",
	"gitlens.statusBar.enabled": false,
	"gitlens.views.commits.avatars": false,
	"gitlens.views.commits.pullRequests.enabled": false,
	"gitlens.views.commits.showBranchComparison": false,
	"go.coverOnTestPackage": false,
	"go.inlayHints.assignVariableTypes": true,
	"go.inlayHints.compositeLiteralFields": true,
	"go.inlayHints.compositeLiteralTypes": true,
	"go.inlayHints.constantValues": true,
	"go.inlayHints.functionTypeParameters": true,
	"go.inlayHints.parameterNames": true,
	"go.inlayHints.rangeVariableTypes": true,
	"go.showWelcome": false,
	"go.tasks.provideDefault": false,
	"go.testExplorer.enable": false,
	"go.toolsManagement.autoUpdate": true,
	"html.format.templating": true,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"launch": {
		"configurations": [
			{
				"console": "integratedTerminal",
				"name": "Debug - Node - npm start",
				"request": "launch",
				"runtimeArgs": [
					"run-script",
					"start"
				],
				"runtimeExecutable": "npm",
				"skipFiles": [
					"<node_internals>/**"
				],
				"type": "node"
			}
		],
		"version": "0.2.0"
	},
	"macros": {
		"collapseAndClose": [
			"workbench.files.action.collapseExplorerFolders",
			"workbench.action.closeAllEditors"
		]
	},
	"merge-conflict.autoNavigateNextConflict.enabled": true,
	"prettier.bracketSameLine": true,
	"prettier.jsxSingleQuote": true,
	"prettier.printWidth": 120,
	"prettier.singleQuote": true,
	"prettier.trailingComma": "none",
	"prettier.useTabs": true,
	"projectManager.confirmSwitchOnActiveWindow": "always",
	"projectManager.showParentFolderInfoOnDuplicates": true,
	"projectManager.supportSymlinksOnBaseFolders": true,
	"rainbow_csv.enable_context_menu_head": true,
	"rainbow_csv.enable_context_menu_tail": true,
	"rainbow_csv.rbql_with_headers_by_default": true,
	"redhat.telemetry.enabled": false,
	"scm.diffDecorationsIgnoreTrimWhitespace": "true",
	"scss.format.preserveNewLines": false,
	"scss.format.spaceAroundSelectorSeparator": true,
	"scss.lint.compatibleVendorPrefixes": "warning",
	"scss.lint.duplicateProperties": "warning",
	"scss.lint.float": "warning",
	"scss.lint.hexColorLength": "warning",
	"search.showLineNumbers": true,
	"security.promptForLocalFileProtocolHandling": false,
	"security.workspace.trust.enabled": false,
	"security.workspace.trust.untrustedFiles": "open",
	"task.problemMatchers.neverPrompt": {
		"npm": true
	},
	"terminal.external.osxExec": "iTerm.app",
	"terminal.integrated.allowChords": false,
	"terminal.integrated.allowedLinkSchemes": [
		"file",
		"http",
		"https",
		"mailto",
		"vscode",
		"vscode-insiders",
		"raycast",
		"hammerspoon",
		"docker-desktop"
	],
	"terminal.integrated.drawBoldTextInBrightColors": false,
	"terminal.integrated.enableMultiLinePasteWarning": "never",
	"terminal.integrated.fontSize": 15,
	"terminal.integrated.scrollback": 2000,
	"terminal.integrated.shellIntegration.enabled": true,
	"terminal.integrated.tabs.enabled": false,
	"typescript.inlayHints.enumMemberValues.enabled": true,
	"typescript.inlayHints.parameterNames.enabled": "all",
	"typescript.inlayHints.parameterTypes.enabled": true,
	"typescript.inlayHints.propertyDeclarationTypes.enabled": true,
	"typescript.inlayHints.variableTypes.enabled": true,
	"typescript.updateImportsOnFileMove.enabled": "never",
	"window.autoDetectColorScheme": true,
	"window.closeWhenEmpty": true,
	"window.commandCenter": false,
	"window.confirmBeforeClose": "keyboardOnly",
	"window.customTitleBarVisibility": "never",
	"window.nativeTabs": true,
	"window.restoreWindows": "preserve",
	"window.systemColorTheme": "auto",
	"window.title": "${rootName}",
	"window.titleBarStyle": "native",
	"window.zoomPerWindow": false,
	"workbench.activityBar.location": "top",
	"workbench.colorCustomizations": {
		"statusBar.background": "#1A1A1A",
		"statusBar.debuggingBackground": "#1A1A1A",
		"statusBar.noFolderBackground": "#1A1A1A",
		"terminal.ansiBlack": "#2C2F33",
		"terminal.ansiBlue": "#405F74",
		"terminal.ansiBrightBlack": "#efefef",
		"terminal.ansiBrightBlue": "#405F74",
		"terminal.ansiBrightCyan": "#4E7D78",
		"terminal.ansiBrightGreen": "#8F9547",
		"terminal.ansiBrightMagenta": "#714E76",
		"terminal.ansiBrightRed": "#802426",
		"terminal.ansiBrightWhite": "#B7BBB7",
		"terminal.ansiBrightYellow": "#D7A24D",
		"terminal.ansiCyan": "#4E7D78",
		"terminal.ansiGreen": "#01A252",
		"terminal.ansiMagenta": "#714E76",
		"terminal.ansiRed": "#802426",
		"terminal.ansiWhite": "#B7BBB7",
		"terminal.ansiYellow": "#D7A24D",
		"terminal.background": "#27292c",
		"terminal.foreground": "#B7BBB7",
		"terminalCursor.background": "#B7BBB7",
		"terminalCursor.foreground": "#B7BBB7"
	},
	"workbench.colorTheme": "Tomorrow Night",
	"workbench.editor.customLabels.patterns": {
		"**/.*": "${dirname}/${filename}",
		"**/*.*": "${dirname}/${filename}.${extname}"
	},
	"workbench.editor.empty.hint": "hidden",
	"workbench.editor.limit.enabled": true,
	"workbench.editor.limit.excludeDirty": true,
	"workbench.editor.limit.perEditorGroup": true,
	"workbench.editor.pinnedTabSizing": "shrink",
	"workbench.editor.tabActionCloseVisibility": false,
	"workbench.editorAssociations": {
		"*.svg": "default"
	},
	"workbench.fontAliasing": "auto",
	"workbench.layoutControl.enabled": false,
	"workbench.list.smoothScrolling": true,
	"workbench.preferredDarkColorTheme": "Tomorrow Night",
	"workbench.preferredHighContrastLightColorTheme": "Tomorrow Night",
	"workbench.preferredLightColorTheme": "Tomorrow Night",
	"workbench.sideBar.location": "right",
	"workbench.startupEditor": "newUntitledFile",
	"workbench.statusBar.visible": true,
	"workbench.tree.indent": 12,
	"workbench.tree.renderIndentGuides": "onHover",
	"window.zoomLevel": -2,
	"github.copilot.nextEditSuggestions.enabled": true,
	"github.copilot.chat.languageContext.fix.typescript.enabled": true,
	"github.copilot.chat.languageContext.inline.typescript.enabled": true,
	"github.copilot.chat.languageContext.typescript.enabled": true
}
