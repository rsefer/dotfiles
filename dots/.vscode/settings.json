{
	"[php]": {
		"editor.defaultFormatter": "kokororin.vscode-phpfmt"
	},
	"[python]": {
		"editor.defaultFormatter": "ms-python.python"
	},
	"aws.codeWhisperer.shareCodeWhispererContentWithAWS": false,
	"aws.suppressPrompts": {
		"codeWhispererConnectionExpired": true
	},
	"aws.telemetry": false,
	"bookmarks.useWorkaroundForFormatters": true,
	"breadcrumbs.enabled": false,
	"breadcrumbs.symbolSortOrder": "type",
	"colorHelper.resident": true,
	"debug.javascript.autoAttachFilter": "smart",
	"diffEditor.ignoreTrimWhitespace": false,
	"diffEditor.renderSideBySide": false,
	"editor.accessibilitySupport": "off",
	"editor.bracketPairColorization.enabled": true,
	"editor.codeLens": false,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.fontSize": 13,
	"editor.guides.bracketPairs": true,
	"editor.hover.delay": 2500,
	"editor.hover.enabled": true,
	"editor.inlineSuggest.enabled": true,
	"editor.linkedEditing": true,
	"editor.minimap.maxColumn": 60,
	"editor.minimap.renderCharacters": false,
	"editor.minimap.size": "fit",
	"editor.parameterHints.enabled": false,
	"editor.renderLineHighlight": "all",
	"editor.renderWhitespace": "selection",
	"editor.scrollBeyondLastLine": false,
	"editor.stickyScroll.enabled": true,
	"editor.tabCompletion": "on",
	"editor.tabSize": 2,
	"editor.wordWrap": "on",
	"editor.wordWrapColumn": 120,
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"extensions.ignoreRecommendations": true,
	"files.associations": {
		"*.njk": "html"
	},
	"files.hotExit": "onExitAndWindowClose",
	"files.insertFinalNewline": true,
	"fontAwesomeAutocomplete.disableTriggerWordAutoClearPatterns": [
		"**/*"
	],
	"fontAwesomeAutocomplete.patterns": [
		"**/*"
	],
	"git.autofetch": true,
	"git.confirmSync": false,
	"git.enableSmartCommit": true,
	"githubIssues.queries": [
		{
			"label": "Open Issues",
			"query": "repo:${owner}/${repository} state:open"
		}
	],
	"githubPullRequests.createOnPublishBranch": "never",
	"githubPullRequests.pullBranch": "never",
	"githubPullRequests.queries": [],
	"javascript.updateImportsOnFileMove.enabled": "always",
	"launch": {
		"configurations": [
			{
				"console": "integratedTerminal",
				"name": "Debug - Node - npm start",
				"request": "launch",
				"runtimeArgs": [
					"run-script",
					"start"
				],
				"runtimeExecutable": "npm",
				"skipFiles": [
					"<node_internals>/**"
				],
				"type": "node"
			}
		],
		"version": "0.2.0"
	},
	"prettier.jsxSingleQuote": true,
	"prettier.singleQuote": true,
	"prettier.useTabs": true,
	"python.showStartPage": false,
	"rainbow_csv.enable_context_menu_head": true,
	"rainbow_csv.enable_context_menu_tail": true,
	"rainbow_csv.rbql_with_headers_by_default": true,
	"security.workspace.trust.enabled": false,
	"security.workspace.trust.untrustedFiles": "open",
	"terminal.external.osxExec": "iTerm.app",
	"terminal.integrated.allowChords": false,
	"terminal.integrated.drawBoldTextInBrightColors": false,
	"terminal.integrated.enableMultiLinePasteWarning": false,
	"terminal.integrated.experimentalLinkProvider": false,
	"terminal.integrated.experimentalUseTitleEvent": true,
	"terminal.integrated.rendererType": "auto",
	"terminal.integrated.scrollback": 2000,
	"terminal.integrated.shellIntegration.enabled": true,
	"terminal.integrated.tabs.enabled": false,
	"typescript.updateImportsOnFileMove.enabled": "never",
	"window.autoDetectColorScheme": true,
	"window.closeWhenEmpty": true,
	"window.nativeTabs": true,
	"window.restoreWindows": "preserve",
	"window.titleBarStyle": "native",
	"workbench.colorCustomizations": {
		"statusBar.background": "#1A1A1A",
		"statusBar.debuggingBackground": "#1A1A1A",
		"statusBar.noFolderBackground": "#1A1A1A",
		"terminal.ansiBlack": "#2C2F33",
		"terminal.ansiBlue": "#405F74",
		"terminal.ansiBrightBlack": "#efefef",
		"terminal.ansiBrightBlue": "#405F74",
		"terminal.ansiBrightCyan": "#4E7D78",
		"terminal.ansiBrightGreen": "#8F9547",
		"terminal.ansiBrightMagenta": "#714E76",
		"terminal.ansiBrightRed": "#802426",
		"terminal.ansiBrightWhite": "#B7BBB7",
		"terminal.ansiBrightYellow": "#D7A24D",
		"terminal.ansiCyan": "#4E7D78",
		"terminal.ansiGreen": "#01A252",
		"terminal.ansiMagenta": "#714E76",
		"terminal.ansiRed": "#802426",
		"terminal.ansiWhite": "#B7BBB7",
		"terminal.ansiYellow": "#D7A24D",
		"terminal.background": "#27292c",
		"terminal.foreground": "#B7BBB7",
		"terminalCursor.background": "#B7BBB7",
		"terminalCursor.foreground": "#B7BBB7"
	},
	"workbench.colorTheme": "Tomorrow Night",
	"workbench.editor.untitled.hint": "hidden",
	"workbench.sideBar.location": "right",
	"workbench.startupEditor": "newUntitledFile",
	"workbench.statusBar.visible": true,
	"workbench.tree.indent": 12,
	"workbench.tree.renderIndentGuides": "onHover"
}
